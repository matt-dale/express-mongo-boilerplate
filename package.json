{
	"name": "express-mongo-boilerplate",
	"version": "1.0.0",
	"description": "A starting point for a Node.js + Express + MongoDB + TypeScript project",
	"main": "index.js",
	"scripts": {
		"start": "node build/src/index.ts",
		"build": "tsc",
