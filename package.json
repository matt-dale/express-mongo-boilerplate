	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.15",
		"@types/express-rate-limit": "^6.0.0",
		"@types/jest": "^29.2.6",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/node": "^18.11.18",
		"@types/supertest": "^2.0.12",
		"dotenv": "^16.0.3",
		"jest": "^29.3.1",
		"nodemon": "^2.0.20",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.8.2",
		"tslint": "^6.1.3",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"express": "^4.18.2",
		"express-rate-limit": "^6.7.0",
		"express-validator": "^6.14.2",
		"helmet": "^6.0.1",
		"jsonwebtoken": "^9.0.0",
		"module-alias": "^2.2.2",
		"mongodb": "^4.13.0",
		"pretty-error": "^4.0.0",
